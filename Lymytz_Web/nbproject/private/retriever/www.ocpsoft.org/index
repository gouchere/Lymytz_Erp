	window.setInterval(function() {
		var c = jQuery(".ocpsoft-blinking-cursor");
		if (c.data('on') === true) {
			c.hide();
			c.data('on', false);
		} else {
			c.data('on', true);
			c.show();
		}
	}, 500)
