	<parameter name="ETAT" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
	yvs_base_tiers."code_tiers" AS yvs_base_tiers_code_tiers,
	CONCAT(yvs_base_tiers."nom", ' ', yvs_base_tiers."prenom") AS yvs_base_tiers_nom_prenom,
	yvs_base_tiers."email" AS yvs_base_tiers_email,
	yvs_base_tiers."tel" AS yvs_base_tiers_telephone,
	yvs_com_client."code_client" AS yvs_com_client_code_client,
	y.montant AS yvs_com_client_chiffre_affaire,
	y.avance AS yvs_com_client_reglement,
	y.acompte AS yvs_com_client_acompte,
	y.credit AS yvs_com_client_credit,
	y.reste AS yvs_com_client_solde,
	y.solde_initial AS yvs_com_client_solde_initial,
	y.rang AS yvs_com_client_rang,
	yvs_base_plan_comptable."num_compte" AS yvs_base_plan_comptable_num_compte,
	yvs_dictionnaire_pays."libele" AS yvs_dictionnaire_pays_libelle,
	yvs_dictionnaire_ville."libele" AS yvs_dictionnaire_ville_libelle,
	yvs_dictionnaire_secteur."libele" AS yvs_dictionnaire_secteur_libelle,
	yvs_base_categorie_comptable."designation" AS yvs_base_categorie_comptable_designation
FROM
	public.com_et_dashboard_client($P{SOCIETE}, $P{AGENCE}, $P{CLIENT}, $P{DATE_DEBUT}, $P{DATE_FIN}, 'M', 'C', false, true, true) y
INNER JOIN public."yvs_com_client" yvs_com_client ON yvs_com_client.id = y.id
INNER JOIN public."yvs_base_tiers" yvs_base_tiers ON yvs_com_client.tiers = yvs_base_tiers.id
LEFT JOIN public."yvs_base_plan_comptable" yvs_base_plan_comptable ON yvs_com_client.compte = yvs_base_plan_comptable.id
LEFT JOIN public."yvs_dictionnaire" yvs_dictionnaire_pays ON yvs_base_tiers.pays = yvs_dictionnaire_pays.id
LEFT JOIN public."yvs_dictionnaire" yvs_dictionnaire_ville ON yvs_base_tiers.ville = yvs_dictionnaire_ville.id
LEFT JOIN public."yvs_dictionnaire" yvs_dictionnaire_secteur ON yvs_base_tiers.secteur = yvs_dictionnaire_secteur.id
LEFT JOIN public."yvs_base_categorie_comptable" yvs_base_categorie_comptable ON yvs_com_client.categorie_comptable = yvs_base_categorie_comptable.id]]>
	</queryString>
