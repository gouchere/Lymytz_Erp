	</parameter>
	<queryString>
		<![CDATA[SELECT
     yvs_base_point_vente."code" AS yvs_base_point_vente_code,
     yvs_base_point_vente."libelle" AS yvs_base_point_vente_libelle,
     yvs_base_point_vente."agence" AS yvs_base_point_vente_agence,
     yvs_com_client."code_client" AS yvs_com_client_code_client,
     yvs_com_doc_ventes."nom_client" AS yvs_com_client_nom,
     yvs_com_doc_ventes."statut" AS yvs_com_doc_ventes_statut,
     yvs_com_doc_ventes."nature" AS yvs_com_doc_ventes_nature,
     concat(yvs_com_entete_doc_vente."date_entete", ' ',yvs_com_doc_ventes."heure_doc"):: timestamp AS yvs_com_entete_doc_vente_date_entete,
     yvs_com_doc_ventes."num_doc" AS yvs_com_doc_ventes_num_doc,
     concat(yvs_com_doc_ventes."date_livraison", ' ',yvs_com_doc_ventes_informations."heure_debut") AS yvs_com_doc_ventes_date_debut,
     concat(yvs_com_doc_ventes_informations."date_fin", ' ',yvs_com_doc_ventes_informations."heure_fin") AS yvs_com_doc_ventes_date_fin,
     yvs_base_tiers."adresse" AS yvs_base_tiers_adresse,
     yvs_dictionnaire."libele" AS yvs_dictionnaire_pays,
     yvs_base_tiers_telephone."numero" AS yvs_base_tiers_telephone_numero,
     yvs_base_model_reglement."description" AS yvs_base_model_reglement_description,
     yvs_agences."id" AS yvs_agences_id,
     yvs_societes."id" AS yvs_societes_id,
     yvs_societes."name" AS yvs_societes_name,
     COALESCE(yvs_societes."email", '---') AS yvs_societes_email,
     COALESCE(yvs_societes."forme_juridique", '---') AS yvs_societes_forme_juridique,
     COALESCE(yvs_societes."capital", 0) AS yvs_societes_capital,
     COALESCE(yvs_societes."site_web", '----') AS yvs_societes_site_web,
     COALESCE(yvs_societes."numero_contribuable", '----') AS yvs_societes_numero_contribuable,
     COALESCE(yvs_societes."numero_registre_comerce", '----') AS yvs_societes_numero_registre_comerce,
     COALESCE(yvs_base_tranche_reglement."interval_jour", 0) AS yvs_base_tranche_reglement_interval_jour,
     public.get_ttc_vente(yvs_com_doc_ventes."id") as yvs_com_doc_ventes_ca,
     COALESCE((SELECT sum(t.montant) from public.get_taxe_vente(yvs_com_doc_ventes."id") t), 0) as yvs_com_doc_ventes_taxe,
     COALESCE((SELECT SUM(m."montant") FROM "public"."yvs_com_cout_sup_doc_vente" m WHERE m."doc_vente" = $P{ID}), 0) AS yvs_com_doc_ventes_penalites,
     COALESCE((SELECT SUM(public.get_ttc_vente(m."id")) FROM "public"."yvs_com_doc_ventes" m INNER JOIN "public"."yvs_com_entete_doc_vente" e ON m.entete_doc = e.id WHERE m."client" = yvs_com_doc_ventes.client AND m.type_doc = 'FV' AND m.statut = 'V' AND m.statut_regle != 'P' AND m.id != yvs_com_doc_ventes."id" AND e.date_entete < yvs_com_entete_doc_vente."date_entete"), 0) AS yvs_com_doc_ventes_impayes,
     (select t.numero from public.yvs_base_tiers_document t where t.tiers = yvs_com_client."tiers" AND t.type = 'NUI' LIMIT 1) as yvs_com_client_numero_contribuable,
     (select t.numero from public.yvs_base_tiers_document t where t.tiers = yvs_com_client."tiers" AND t.type = 'RCC' LIMIT 1) as yvs_com_client_numero_registre_commerce,
     yvs_print_header."nom" AS yvs_print_header_nom,
     COALESCE(yvs_print_header."model", 'header_v1') AS yvs_print_header_model,
     COALESCE(yvs_print_footer."model", 'footer_v1') AS yvs_print_footer_model,
     COALESCE(yvs_print_facture_vente."view_date_facture", TRUE) AS yvs_print_facture_vente_view_date_facture,
     COALESCE(yvs_print_facture_vente."view_numero_facture", TRUE) AS yvs_print_facture_vente_view_numero_facture,
     COALESCE(yvs_print_facture_vente."view_taxe_facture", TRUE) AS yvs_print_facture_vente_view_taxe_facture,
     COALESCE(yvs_print_facture_vente."view_cout_facture", TRUE) AS yvs_print_facture_vente_view_cout_facture,
     COALESCE(yvs_print_facture_vente."view_service_facture", TRUE) AS yvs_print_facture_vente_view_service_facture,
     COALESCE(yvs_print_facture_vente."view_reglement_facture", TRUE) AS yvs_print_facture_vente_view_reglement_facture,
     COALESCE(yvs_print_facture_vente."view_statut_facture", TRUE) AS yvs_print_facture_vente_view_statut_facture,
     COALESCE(yvs_print_facture_vente."view_name_vendeur", TRUE) AS yvs_print_facture_vente_view_name_vendeur,
     COALESCE(yvs_print_facture_vente."view_point_vente", TRUE) AS yvs_print_facture_vente_view_point_vente,
     COALESCE(yvs_print_facture_vente."view_name_client", TRUE) AS yvs_print_facture_vente_view_name_client,
     COALESCE(yvs_print_facture_vente."view_phone_client", TRUE) AS yvs_print_facture_vente_view_phone_client,
     COALESCE(yvs_print_facture_vente."view_adresse_client", TRUE) AS yvs_print_facture_vente_view_adresse_client,
     COALESCE(yvs_print_facture_vente."view_nui_client", TRUE) AS yvs_print_facture_vente_view_nui_client,
     COALESCE(yvs_print_facture_vente."view_rcc_client", TRUE) AS yvs_print_facture_vente_view_rcc_client,
     COALESCE(yvs_print_facture_vente."view_signature_vendeur", TRUE) AS yvs_print_facture_vente_view_signature_vendeur,
     COALESCE(yvs_print_facture_vente."view_signature_caissier", TRUE) AS yvs_print_facture_vente_view_signature_caissier,
     COALESCE(yvs_print_facture_vente."view_signature_client", TRUE) AS yvs_print_facture_vente_view_signature_client,
     COALESCE(yvs_print_facture_vente."view_image_payer", TRUE) AS yvs_print_facture_vente_view_image_payer,
     COALESCE(yvs_print_facture_vente."view_image_livrer", TRUE) AS yvs_print_facture_vente_view_image_livrer

FROM     "public"."yvs_base_point_vente" yvs_base_point_vente
     INNER JOIN "public"."yvs_com_creneau_point" yvs_com_creneau_point ON yvs_base_point_vente."id" = yvs_com_creneau_point."point"
     INNER JOIN "public"."yvs_com_creneau_horaire_users" yvs_com_creneau_horaire_users ON yvs_com_creneau_point."id" = yvs_com_creneau_horaire_users."creneau_point"
     INNER JOIN "public"."yvs_com_entete_doc_vente" yvs_com_entete_doc_vente ON yvs_com_creneau_horaire_users."id" = yvs_com_entete_doc_vente."creneau"
     INNER JOIN "public"."yvs_users" yvs_users ON yvs_com_creneau_horaire_users."users" = yvs_users."id"
     INNER JOIN "public"."yvs_com_doc_ventes" yvs_com_doc_ventes ON yvs_com_entete_doc_vente."id" = yvs_com_doc_ventes."entete_doc"
     LEFT JOIN "public"."yvs_com_doc_ventes_informations" yvs_com_doc_ventes_informations ON yvs_com_doc_ventes."id" = yvs_com_doc_ventes_informations."facture"
     INNER JOIN "public"."yvs_com_client" yvs_com_client ON yvs_com_doc_ventes."client" = yvs_com_client."id"
     INNER JOIN "public"."yvs_base_tiers" yvs_base_tiers ON yvs_com_client."tiers" = yvs_base_tiers."id"
     LEFT JOIN "public"."yvs_base_tiers_telephone" yvs_base_tiers_telephone ON yvs_base_tiers."id" = yvs_base_tiers_telephone."tiers"
     INNER JOIN "public"."yvs_agences" yvs_agences ON yvs_agences."id" = yvs_base_point_vente."agence"
     INNER JOIN "public"."yvs_societes" yvs_societes ON yvs_societes."id" = yvs_agences."societe"
     INNER JOIN "public"."yvs_dictionnaire" yvs_dictionnaire ON yvs_dictionnaire."id" = yvs_base_tiers."pays"
     LEFT JOIN "public"."yvs_base_model_reglement" yvs_base_model_reglement ON yvs_com_doc_ventes."model_reglement" = yvs_base_model_reglement."id"
     LEFT JOIN "public"."yvs_base_tranche_reglement" yvs_base_tranche_reglement ON yvs_com_doc_ventes."model_reglement" = yvs_base_tranche_reglement."model"
     LEFT JOIN "public"."yvs_print_facture_vente" yvs_print_facture_vente ON (yvs_agences."societe" = yvs_print_facture_vente."societe" AND ((TRIM(COALESCE($P{MODEL}, '')) != '' AND yvs_print_facture_vente."nom" = $P{MODEL}) OR (TRIM(COALESCE($P{MODEL}, '')) = '' AND yvs_print_facture_vente."defaut" IS TRUE)))
     LEFT JOIN "public"."yvs_print_header" yvs_print_header ON yvs_print_header."id" = yvs_print_facture_vente."header"
     LEFT JOIN "public"."yvs_print_footer" yvs_print_footer ON yvs_print_footer."id" = yvs_print_facture_vente."footer"
WHERE
     yvs_com_doc_ventes."id" = $P{ID}
ORDER BY yvs_base_tiers_telephone."principal" DESC
LIMIT 1]]>
	</queryString>
