	<queryString>
		<![CDATA[SELECT
     yvs_societes."id" AS yvs_societes_id,
     yvs_societes."adress_siege" AS yvs_societes_adress_siege,
     yvs_societes."code_postal" AS yvs_societes_code_postal,
